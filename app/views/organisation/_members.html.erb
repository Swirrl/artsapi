<table class='table table-striped'>
  <thead>
    <th>Member</th>
    <th>Number of known Connections</th>
  </thead>
  <tbody>
    <% @presenter.members_data.each do |member| %>
      <tr>
        <td>
          <a href="<%= member[0] %>">
            <%= member[1] %>
          </a>
        </td>
        <td><%= member[2] %></td>
      </tr>
    <% end %>
  </tbody>
</table>