<% provide :title, @presenter.title; provide :title_editable, true %>

<% if @presenter.person? %>

  <%= render 'person/summary' %>
  <%= render 'person/tabbed_table' %>

<% elsif @presenter.organisation? %>

  <%= render 'organisation/summary' %>
  <%= render 'organisation/tabbed_table' %>

<% else %>

  <%= render 'shared/data' %>

<% end %>
