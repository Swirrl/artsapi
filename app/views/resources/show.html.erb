<% provide :title, @presenter.title %>
<h1>Viewing <%= @presenter.title %></h1>

<table>
  <thead>
    <th>Description</th>
    <th>URI</th>
    <th>Object</th>
  </thead>
  <tbody>
    <% @presenter.fields.each do |field| %>
      <tr>
        <td><%= field[0] %></td>
        <td><%= field[1] %></td>
        <td><%= field[2] %></td>
      </tr>
    <% end %>
  </tbody>
</table>