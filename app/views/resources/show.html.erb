<% provide :title, @presenter.title %>

<div id='resource_uri' data-resource-uri='<%= @presenter.resource_uri %>'></div>

<% if @presenter.person? %>

  <%= render 'person/summary' %>
  <%= render 'person/tabbed_table' %>

<% elsif @presenter.organisation? %>

  <%= render 'organisation/summary' %>
  <%= render 'organisation/tabbed_table' %>

<% else %>

  <%= render 'shared/data' %>

<% end %>
