<div class='row no-margins'>
  <div class='col-md-3'>
    <h1 class='text-center'>
      <%= yield(:title) %>
      <%= inline_edit_link_for(@presenter.resource_uri) if @presenter && @presenter.respond_to?(:resource_uri) && :title_editable %>
    </h1>
  </div>
  <div class='col-md-5'></div>
  <div class='col-md-4'>
    <%= image_tag 'aa-logo.png', width: 100, height: 100, id: 'main-logo' %>
  </div>
</div>